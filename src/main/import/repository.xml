<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <tfl-internet-templates j:dependencies="default search"
                              j:installedModules="tfl-internet-templates"
                              j:modulePriority="0"
                              j:moduleType="templatesSet"
                              j:title="tfl-internet-templates"
                              jcr:mixinTypes="jmix:hasExternalProviderExtension"
                              jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-tfl-internet-templates-1.0-SNAPSHOT">
                    <src jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                            <resources jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                                <META-INF jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:metaInfFolder">
                                    <definitions.cnd jcr:mixinTypes="jmix:sourceControl"
                                         jcr:primaryType="jnt:definitionFile"/>
                                </META-INF>
                            </resources>
                        </main>
                    </src>
                </sources>
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder">
                                <bg.jpg j:height="636"
                             j:width="1280"
                             j:xresolution="100 dots"
                             j:yresolution="100 dots"
                             jcr:mixinTypes="jmix:exif jmix:image"
                             jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                </bg.jpg>
                            </default>
                        </themes>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/tfl-internet-templates/templates/files/themes/default"
                  j:view="tfl-internet"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                        <home j:view="home-tfl-internet" jcr:primaryType="jnt:pageTemplate">
                            <top-header jcr:primaryType="jnt:contentList">
                                <top-header-area j:allowedTypes="bgernt:topHeader"
                                      j:limitedAbsoluteAreaEdit="false"
                                      j:numberOfItems="1"
                                      jcr:primaryType="jnt:absoluteArea"/>
                            </top-header>
                        </home>
                        <simple jcr:primaryType="jnt:pageTemplate">
                            <main-navigation jcr:primaryType="jnt:contentList">
                                <main-navigation-area j:allowedTypes="bgernt:simpleMenu"
                                           j:limitedAbsoluteAreaEdit="false"
                                           j:numberOfItems="1"
                                           j:subNodesView="main"
                                           jcr:primaryType="jnt:absoluteArea"/>
                            </main-navigation>
                            <left-navigation jcr:primaryType="jnt:contentList">
                                <left-menu-area j:allowedTypes="bgernt:leftMenu"
                                     j:limitedAbsoluteAreaEdit="false"
                                     j:numberOfItems="1"
                                     jcr:primaryType="jnt:absoluteArea"/>
                            </left-navigation>
                        </simple>
                        <top-header jcr:primaryType="jnt:contentList">
                            <top-header-area j:allowedTypes="bgernt:topHeader"
                                   j:limitedAbsoluteAreaEdit="false"
                                   j:numberOfItems="1"
                                   jcr:primaryType="jnt:absoluteArea"/>
                        </top-header>
                        <myHomePageTemplate j:themeName="default"
                                   j:view="home-tfl-internet"
                                   jcr:mixinTypes="jmix:theme"
                                   jcr:primaryType="jnt:pageTemplate">
                            <top-header jcr:primaryType="jnt:contentList">
                                <top-header-area j:allowedTypes="bgernt:topHeader"
                                      j:limitedAbsoluteAreaEdit="false"
                                      j:numberOfItems="1"
                                      jcr:primaryType="jnt:absoluteArea"/>
                            </top-header>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="myHomePageTemplate"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <homemenu defaultBackground="/modules/tfl-internet-templates/templates/files/themes/default/bg.jpg"
                               jcr:primaryType="bgernt:homeMenu"/>
                                <panopticmenu j:view="load"
                                   jcr:mixinTypes="jmix:renderable"
                                   jcr:primaryType="bgernt:panopticMenu">
                                    <panopticmenupage jcr:primaryType="bgernt:panopticMenuPage"
                                          page="/modules/tfl-internet-templates/home"
                                          photo="/modules/tfl-internet-templates/templates/files/themes/default/bg.jpg"
                                          title="Home"/>
                                    <panopticmenupage-1 jcr:primaryType="bgernt:panopticMenuPage"
                                            page="/modules/tfl-internet-templates/home"
                                            photo="/modules/tfl-internet-templates/templates/files/themes/default/bg.jpg"
                                            title="Home"/>
                                    <panopticmenupage-2 jcr:primaryType="bgernt:panopticMenuPage"
                                            page="/modules/tfl-internet-templates/home"
                                            photo="/modules/tfl-internet-templates/templates/files/themes/default/bg.jpg"
                                            title="Home"/>
                                </panopticmenu>
                            </pagecontent>
                        </myHomePageTemplate>
                    </base>
                    <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="tfl-internet-templates"
                              jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                        </pagecontent>
                    </content-template>
                </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
            </tfl-internet-templates>
    </modules>
</content>